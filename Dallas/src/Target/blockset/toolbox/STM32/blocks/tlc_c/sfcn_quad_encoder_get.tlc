%implements  sfcn_quad_encoder_get "C"

%% add required include files
%<LibAddToCommonIncludes("quadencoder.h")>


%% Function: Start ==========================================================
%%
%% Purpose:
%%      Code generation for initialization
%%
%function Start(block, system) Output
  %%
  /* configure the quadrature encoder module */
  QuadEncConfigure(%<block.RTWdata.moduleID>, %<block.RTWdata.config>);
  %%
%endfunction

%% Function: Output ==========================================================
%%
%% Purpose:
%%      Code generation for signal output
%%
%function Outputs(block, system) Output
	%%
    /* determine if the counter needs a reset  */
    if (%<LibBlockInputSignal(0, "", "", 0)> > 0)
    {
      QuadEncResetCounter(%<block.RTWdata.moduleID>);
    }
    /* read counter from the quadrature encoder module */
    %<LibBlockOutputSignal(0, "", "", 0)> = QuadEncGetCounter(%<block.RTWdata.moduleID>);
  %%
%endfunction



