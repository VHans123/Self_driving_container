%implements  sfcn_udp_server_count "C"

%% add required include files
%<LibAddToCommonIncludes("udp_server.h")>


%% Function: Output ==========================================================
%%
%% Purpose:
%%      Code generation for signal output
%%
%function Outputs(block, system) Output
	%%
    /* read how many bytes are currently available in the UDP server's receive buffer */
    %<LibBlockOutputSignal(0, "", "", 0)> = UdpServerGetRxCount();
  %%
%endfunction



