%implements  sfcn_analog_out "C"

%% add required include files
%<LibAddToCommonIncludes("anout.h")>


%% Function: Start ==========================================================
%%
%% Purpose:
%%      Code generation for initialization
%%
%function Start(block, system) Output
  %%
  /* configure the analog output */
  DAC_init(%<block.RTWdata.DACchannel>);
  %%
%endfunction

%% Function: Update ==========================================================
%%
%% Purpose:
%%      Code generation for signal input
%%
%function Update(block, system) Output
	%%
    /* update analog output */
	DAC_update( %<LibBlockInputSignal(0, "", "", 0)>, %<block.RTWdata.DACchannel>);
%endfunction



