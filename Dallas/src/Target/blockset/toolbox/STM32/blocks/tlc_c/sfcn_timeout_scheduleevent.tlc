%implements  sfcn_timeout_scheduleevent "C"

%% add required include files
%<LibAddToCommonIncludes("timeout.h")>


%% Function: Start ==========================================================
%%
%% Purpose:
%%      Code generation for initialization
%%
%function Start(block, system) Output
  %%
  %%
%endfunction


%% Function: Update ==========================================================
%%
%% Purpose:
%%      Code generation for signal input
%%
%function Update(block, system) Output
	%%
  %assign eventCount = LibBlockInputSignal(0, "", "", 0)
  %assign pinAction = LibBlockInputSignal(1, "", "", 0)
  /* schedule the next output compare event */
  TimeoutScheduleCompareEvent(%<block.RTWdata.pin>, %<eventCount>, (tTimeoutAction)%<pinAction>);
  %%
%endfunction



